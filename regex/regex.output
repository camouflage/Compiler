state 0

    0 $accept: . input $end

    $default  reduce using rule 1 (input)

    input  go to state 1


state 1

    0 $accept: input . $end
    2 input: input . line

    $end  shift, and go to state 2
    '\n'  shift, and go to state 3

    '\n'      [reduce using rule 9 (cat)]
    $default  reduce using rule 9 (cat)

    line  go to state 4
    exp   go to state 5
    cat   go to state 6


state 2

    0 $accept: input $end .

    $default  accept


state 3

    3 line: '\n' .

    $default  reduce using rule 3 (line)


state 4

    2 input: input line .

    $default  reduce using rule 2 (input)


state 5

    4 line: exp . '\n'
    5 exp: exp . '|' cat

    '|'   shift, and go to state 7
    '\n'  shift, and go to state 8


state 6

    6 exp: cat .
    7 cat: cat . term
    8    | cat . sign

    CHAR  shift, and go to state 9
    '*'   shift, and go to state 10
    '.'   shift, and go to state 11

    $default  reduce using rule 6 (exp)

    term  go to state 12
    sign  go to state 13


state 7

    5 exp: exp '|' . cat

    $default  reduce using rule 9 (cat)

    cat  go to state 14


state 8

    4 line: exp '\n' .

    $default  reduce using rule 4 (line)


state 9

   10 term: CHAR .

    $default  reduce using rule 10 (term)


state 10

   12 sign: '*' .

    $default  reduce using rule 12 (sign)


state 11

   11 term: '.' .

    $default  reduce using rule 11 (term)


state 12

    7 cat: cat term .

    $default  reduce using rule 7 (cat)


state 13

    8 cat: cat sign .

    $default  reduce using rule 8 (cat)


state 14

    5 exp: exp '|' cat .
    7 cat: cat . term
    8    | cat . sign

    CHAR  shift, and go to state 9
    '*'   shift, and go to state 10
    '.'   shift, and go to state 11

    $default  reduce using rule 5 (exp)

    term  go to state 12
