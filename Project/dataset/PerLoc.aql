create view PerLocOnly as
  select L.Loc as Loca
  from Loc L;

create view Stt as
  extract regex /Washington|Georgia|Virginia/
    on D.text
    return group 0 as Stt
  from Document D;


create view My as
  extract regex /Carter|,/
    on D.text as kk
  from Document D;

create view Loc as
  extract pattern <L.Cap> <PL.Loca>
  return group 0 as Loc
    and group 1 as Cap
    and group 2 as Stt
  from Loc L, PerLocOnly PL;


